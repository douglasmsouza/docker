services:
  - name: reqres
    protocol: https
    host: reqres.in
    port: 80
    connect_timeout: 60000
    read_timeout: 60000
    write_timeout: 60000
    retries: 1
    routes:
      - name: users
        paths:
          - /api/users
        preserve_host: false
        protocols:
          - http
          - https
        regex_priority: 0
        strip_path: true
        plugins: []
